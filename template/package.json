  "private": true,
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.1",
    <%_ if (cypress) { _%>
    "cypress": "^3.4.0",
    <%_ } _%>
    "elm": "^0.19.1-2",
    "elm-hot": "^1.0.1",
    <%_ if (netlify) { _%>
    "netlify-cli": "^2.11.23",
